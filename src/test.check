#test sub_1
unsigned int a = 45;
unsigned int b = 5;
s21_decimal x = {a, 0, 0, 0};
s21_decimal y = {b, 0, 0, 0};
s21_decimal res = {0};
s21_sub(x, y, &res);
unsigned int num_res = a - b;
unsigned int sign_res = 0;
ck_assert_uint_eq(res.bits[0], num_res);
ck_assert_uint_eq(res.bits[3], sign_res);

